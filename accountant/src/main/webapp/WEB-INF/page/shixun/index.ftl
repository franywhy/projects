   <script type="text/javascript" src="${STATIC_COMMON}/js/jquery.js"></script>
   
    <link rel="stylesheet" type="text/css" href="${STATIC_COMMON}/css/shixun.css"/>
    <style type="text/css">
        .c-box .c-box-top{background:none!important;}
        .main-sel-section .main-sel-h2{color:initial;cursor:pointer;}
        div.act,li.act{color: #18aa1f!important;}
        .main-course-box a{color:inherit;cursor:pointer;display:block;}
        /*购买提示*/
        .buy-VIP-popBox{position:fixed;top:50%;left:50%;z-index:9990;z-index:999!important;overflow:hidden;margin-top:-145px;margin-left:-250px;width:500px;height:290px;border-radius:10px;background-color:#fff;-webkit-box-shadow:3px 7px 8px #333;-moz-box-shadow:3px 7px 8px #333;box-shadow:3px 7px 8px #333;display:none;}
        .buy-VIP-popBox .off-pop-btn{float:right;display:inline-block;margin-top:11px;margin-right:10px;width:17px;height:17px;background:url(/static/images/icon/offBox-btn.png) no-repeat left top;cursor:pointer;}
        .buy-VIP-popBox .off-pop-btn:hover{background-position:-26px top;}
        .buy-VIP-popBox .buy-pop-tit{width:100%;height:45px;background-color:#f2f2f2;line-height:45px;}
        .buy-VIP-popBox .buy-pop-tit span{padding-left:43px;background:url(/static/images/icon/tan_icon.png) no-repeat 15px center;color:#2e2e2e;font-size:18px;}
        .buy-VIP-popBox .buy-pop-con{padding:20px 50px;}
        .buy-VIP-popBox .buy-pop-con h2{padding-left:26px;background:url(/static/images/icon/huangguan.png) no-repeat 0 center;color:#2e2e2e;font-weight:400;font-size:15px;line-height:20px;}
        .buy-VIP-popBox .buy-pop-con h2 .active{background:url(/static/images/icon/huangguan1.png) no-repeat 0 center;}
        .buy-VIP-popBox .buy-pop-con h2 .actived{background:url();}
        .buy-VIP-popBox .buy-pop-con p{margin-top:6px;color:#777;font-size:14px;line-height:25px;}
        .buy-VIP-popBox .buy-pop-con p span{color:#ff8b3b;}
        .buy-VIP-popBox .buy-pop-con .is-play-RMB,.buy-VIP-popBox .buy-pop-con .select-scrow,.buy-VIP-popBox .buy-pop-con .select-taocan{margin-top:10px;width:110%;height:35px;color:#555;line-height:35px;}
        .buy-VIP-popBox .buy-pop-con .is-play-RMB span,.buy-VIP-popBox .buy-pop-con .select-scrow span,.buy-VIP-popBox .buy-pop-con .select-taocan span{font-size:15px;}
        .buy-VIP-popBox .buy-pop-con .select-scrow select{width:321px;height:28px;border:1px solid #ccc;font-size:14px;}
        .buy-VIP-popBox .buy-pop-con .select-taocan a{display:inline-block;margin-right:15px;width:64px;height:28px;border:1px solid #2bb6ea;border-radius:2px;color:#2bb6ea;text-align:center;font-size:14px;line-height:28px;}
        .buy-VIP-popBox .buy-pop-con .select-taocan a.active{background-color:#2bb6ea;color:#fff;}
        .buy-VIP-popBox .buy-pop-con .is-play-RMB a{color:#ef7600;font-size:22px;}
        .buy-VIP-popBox .buy-pop-con .buy-VIP-btn{margin-top:25px;width:400px;height:45px;border-radius:4px;background-color:#ccc;color:#fff;text-align:center;line-height:45px;}
        .buy-VIP-popBox .buy-pop-con .buy-VIP-btn.active{background-color:#18aa1f;font-size:16px;cursor:pointer;}
    </style>
	
</head>
<body>
	<!-- 导航栏 -->
	<#include "../commons/header.ftl">
	<script type="text/javascript">
        $('.nav li').eq(3).addClass('act');//头部导航选中状态
    </script>
	<#setting number_format="#">
	<!--内容-->
    <div class="mainCon shixun-mainCon">
		<div class="main-bar-a">
			<div class="w1200">
				<a href="/">首页</a> > 实训
			</div>
		</div>
		<div class="slide-banner">
			<ul class="slide-ul clearfix">
				<li class="slide-li" style="background: url(/static/images/bg/shixun-bg-1.jpg) no-repeat center 0;">
					<h1 class="t-h1">行业实操覆盖全</h1>
					<h1 class="t-h2">工业、商业、旅游业、建筑施工、外贸、物流业、房地产、酒店餐饮、广告业、装饰装修<br />十大热门高薪行业经验，快速轻松掌握。<br />有了它，会计跳槽高薪行业再也不用怕！</h1>
				</li>
				<li class="slide-li" style="background: url(/static/images/bg/shixun-bg-2.jpg) no-repeat center 0;">
					<h1 class="t-h1">拟真操作全过程</h1>
					<h1 class="t-h2">建账 - 做账 - 过账 - 结账 - 报账<br />真实原始单据全过程动手演练</h1>
				</li>
				<li class="slide-li" style="background: url(/static/images/bg/shixun-bg-3.jpg) no-repeat center 0;">
					<h1 class="t-h1">智能化服务</h1>
					<h1 class="t-h2">大数据分析比你更懂你自己<br />学习评估、错误报告和分析<br />无限次，分岗位练习</h1>
				</li>
				<li class="slide-li" style="background: url(/static/images/bg/shixun-bg-4.jpg) no-repeat center 0;">
					<h1 class="t-h1">与工作零距离</h1>
					<h1 class="t-h2">行业精品视频讲解<br />账簿、财务、税务报表齐全<br />会计角色就能完成全套账务核算</h1>
				</li>
			</ul>
			<dl class="slide-dl clearfix">
				<dd class="slide-dd act"></dd>
				<dd class="slide-dd"></dd>
				<dd class="slide-dd"></dd>
				<dd class="slide-dd"></dd>
			</dl>
		</div>
		<div class="main-sel-section f5f5f5">
			<div class="w1200">
				<div class="main-sel-bar clearfix">
					<div class="main-sel-h1 left">账套筛选：</div>
					<div class="main-sel-h4 left">共 <i>10</i> 个课程</div>
				</div>
				<div class="main-sel-bor"></div>
				<div class="main-sel-bar clearfix">
					<div class="main-sel-h1 left">行业账套：</div>
					<div class="main-sel-h2 left act">不限</div>
					<ul class="main-sel-h3 left clearfix"></ul>
				</div>
			</div>
		</div>
		<div class="shixun-main-a">
			<div class="w1200">
				<div class="main-big-box">
					<div class="main-bar-shaixuan clearfix">
						<ul class="main-shaixuan-r right">
							<li><i class="checkGou"></i>仅显示我的帐套</li>
						</ul>
					</div>
					<div class="main-course-box-out">
						<div class="main-course-box clearfix">
                            <#list allProductlist as item>
                            	<div href='javascript:;' data-practiceid='${item.practiceid}' data-visible='true'>
                            		<div class="c-box">
                            			<div class="c-box-top relative">
                            				<img class="" src="http://sx.hqjy.com/resource${item.photo}" />
                            				<div class="c-box-bar-2 absolute">
                            					<#assign num=1000 + item.id + item.practiceid>
                            					<i>${num }</i>人在练习
                            				</div>
                            				<#assign accountid="">
                            				<#list practiceLearnVolist as vo>
                            					<#if vo.practiceid == item.practiceid>
                            						<#assign accountid=vo.accountid+"">
                            					</#if>
                            				</#list>
                            				<div class="slide-box">
                            					<p class='left'><a class='doPractice' href='javascript:;' data-href="http://sx.hqjy.com/app/train/${accountid}" >立即练习</a></p>
                            					<p class="right"><a href="http://sx.hqjy.com/app/probation/${item.probationAccountId}" >免费试用</a></p>
                            				</div>
                            			</div>
                            			<div class="c-box-bot">
                            				<div class="c-box-h1">${item.productname }</div>
                            				<#if practiceids?index_of(item.practiceid+"") != -1>
                            					<div class="c-box-h3">已开通</div>
                            				<#else>
                            					<div class="c-box-h3">免费试用</div>
                            				</#if>
                            				
                            				<div class="c-box-h2">
                            					<p class="left" title="${item.summary }">
                            						<#if item.summary?length gt 35>  
													    ${item.summary?substring(0,35)}...
													<#else>${item.summary}   
													</#if>  
                            					</p>
                            					<div class="clear"></div>
                            				</div>
                            			</div>
                            		</div>
                            	</div>
                            </#list>
						</div>
						<div class="main-course-box clearfix none"></div>
					</div>
				</div>
			</div>
		</div>
	</div>

    <script type="text/javascript" src="/static/head/js/foot.js"></script>
	<div class="buy-VIP-popBox">
        <span class='btn-right off-pop-btn'></span>
        <div class="buy-pop-tit"><span>购买提示</span></div>
        <div class="buy-pop-con">
            <h2 id="buyVIPh2" runat="server">您好，您还没有对应的操作权限</h2>
            <p>购买相关课程即可立即开通，获得专属功能，大神都在使用，您还在等什么呢？</p>
            <div id="buy-VIP-btn" class="buy-VIP-btn active">立即查看</div>
        </div>
    </div>
    <script src="${STATIC_COMMON}/js/shixun.js"></script>
    <script type="text/javascript">
        $(function () {
            //var isLogined = '<%=MemberFacade.IsLogined()%>'.toLowerCase();
            var isLogined = 'true';
            $('.main-sel-h4 i').text($('.main-course-box div[data-practiceid][data-visible="true"]').length);
            //“仅显示我的账套”单击事件
            var practiceids_by_self = '${practiceids}';
            //console.info('practiceids_by_self', practiceids_by_self);
            var practiceids_by_self_arr = practiceids_by_self.split(',');
            $('.main-shaixuan-r i.checkGou').click(function () {
                if ($(this).hasClass('act')) {
                    $(this).removeClass('act');
                    $('.main-course-box div[data-practiceid]').attr('data-visible','true').show();
                }
                else {
                    $(this).addClass('act');
                    $('.main-course-box div[data-practiceid]').attr('data-visible', 'false').hide();
                    if (practiceids_by_self_arr.length > 0) {
                        $.each(practiceids_by_self_arr, function (idx,row) {
                            $('.main-course-box div[data-practiceid="' + row + '"]').attr('data-visible', 'true').show();
                        });
                    }
                }
                //$('.main-sel-h4 i').text($('.main-course-box div[data-practiceid][data-visible="true"]').length);
                $('.main-sel-h2').addClass('act');
                $('.main-sel-h3').find('li').removeClass('act');
            });
            //行业账套分类
            $('.main-big-box').find('.c-box-h1').each(function (idx, row) {
                $('<li>' + $(row).text() + '</li>').appendTo($('.main-sel-h3')).click(function () {
                    $('.main-course-box div[data-practiceid]').attr('data-visible', 'false').hide();
                    var practiceid = $(row).parents('div[data-practiceid]').attr('data-practiceid');
                    $('.main-course-box div[data-practiceid="' + practiceid + '"]').attr('data-visible', 'true').show();
                    $(this).addClass('act').siblings().removeClass('act');
                    $('.main-sel-h2').removeClass('act');
                });
            });
            $('.main-sel-h2').click(function () {
                $('.main-course-box div[data-practiceid]').attr('data-visible', 'true').show();
                $(this).addClass('act');
                $('.main-sel-h3').find('li').removeClass('act');
            });
            //练习、试用
            $('a.doPractice').click(function () {
                if (isLogined == 'false') {
                    $('.btn-login').trigger('click');
                }
                else {
                    //判断有没权限
                    var practiceid = $(this).parents('div[data-practiceid]').attr('data-practiceid');
                    if (practiceids_by_self_arr.indexOf(practiceid) > -1) {
                        window.location = $(this).attr('data-href');
                    }
                    else {
                        $('.buy-VIP-popBox').show();

                    }
                }
            });
            //弹出层 - 关闭
            $('.buy-VIP-popBox .off-pop-btn').click(function () {
                $('.buy-VIP-popBox').hide();
            });
        });
    </script>
</body>
</html>