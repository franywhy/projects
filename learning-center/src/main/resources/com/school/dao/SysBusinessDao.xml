<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.school.dao.SysBusinessDao">
	<select id="queryByBusinessId" resultType="com.school.entity.SysBusinessEntity">
		SELECT * 
		FROM sys_business
		WHERE business_id = #{businessId}
	</select>
</mapper>